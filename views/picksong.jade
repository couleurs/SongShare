extends layout

block content
  .container
    h2 Find a song

  // Search Box
  //#receiver(data-name='#{receiver_username}')
  .container
    .row
      .col-xs-12.col-lg-8
        .input-group
          input#search-bar.form-control(type='text' placeholder='Search')
          span.input-group-btn
            button.btn.btn-default(type='button')
              span.glyphicon.glyphicon-search

  .container-fluid
    .row#yt-results

block scripts
  script(src='/js/songsearch.js')