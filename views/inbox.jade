extends layout

block content
  .container
    h1 Inbox

  .container
    .row
      each request, i in requests
        -console.log(request.receiver_name);
        .col-xs-6.col-sm-3.col-lg-2
          .h4 
            = request.requester_name + ':'
          a(href='/listeningroom/' + request.listeningroom_id + '?videoId=' + request.video_id)
            img.img-responsive.album-selector(src='http://ecx.images-amazon.com/images/I/81tEvdYVSPL._SL1417_.jpg', alt='album art')
            p Listen
